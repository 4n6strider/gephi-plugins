<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <attr name="Windows2\Modes\layoutmode\LayoutTopComponent.wstcref\position" intvalue="100"/>
    <attr name="Windows2\Modes\layoutmode\PreviewSettingsTopComponent.wstcref\position" intvalue="300"/>

    <folder name="Actions">
        <folder name="Window">
            <file name="org-gephi-desktop-streaming-StreamingAction.instance">
                <attr name="component" methodvalue="org.gephi.desktop.streaming.StreamingTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.gephi.desktop.streaming.Bundle#CTL_StreamingAction"/>
                <attr name="iconBase" stringvalue="org/gephi/desktop/streaming/media-stream.png"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="StreamingAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-gephi-desktop-streaming-StreamingAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="StreamingTopComponent.settings" url="StreamingTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="layoutmode">
                <file name="StreamingTopComponent.wstcref" url="StreamingTopComponentWstcref.xml">
                    <attr name="position" intvalue="200"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
